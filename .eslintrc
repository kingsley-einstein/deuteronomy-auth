{
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "off",
    "max-len": "off",
    "no-unused-vars": "error",
    "linebreak-style": "off",
    "camelcase": "off",
    "comma-dangle": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "arrow-body-style": "off",
    "global-require": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base"
}
